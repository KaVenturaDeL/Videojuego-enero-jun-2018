void juego(){
  background(#8BFFD0);
  
  //Casa 
  fill(#CE7F00);
  rect(100,450,800,1000);
  fill(#7ED300);
  rect(0,405,1000,80);
  
  //Nubes
  fill(255);
  ellipse(100, 150, 50, 50);
  ellipse(130, 150, 50, 50);
  ellipse(160, 150, 50, 50);
  rect(95,165,70,10);
  
  ellipse(300, 150, 50, 50);
  ellipse(330, 150, 50, 50);
  ellipse(360, 150, 50, 50);
  rect(295,165,70,10);
  
  ellipse(800, 150, 50, 50);
  ellipse(830, 150, 50, 50);
  ellipse(860, 150, 50, 50);
  rect(795,165,70,10);
  
  ellipse(600, 150, 50, 50);
  ellipse(630, 150, 50, 50);
  ellipse(660, 150, 50, 50);
  rect(595,165,70,10);
  
  //Solesito
  fill(#F0FF50);
  ellipse(500, 200, 100, 100);
  
  fill(0);
  text("Vida 1er Jugador", 20,65);
  text("Vida 2do Jugador", 660,65);
  
  text("Ataque1", 180,550);
  text("1er jugador", 130,580);
  text("Press K", 180,610);
  
  text("Ataque2", 650,550);
  text("2do jugador", 600,580);
    text("Press L", 650,610);
  
  
  //Barra de vida
  rect(370, 50, v, 20);
  rect(510, 50, b, 20);
  
  pushMatrix();
  translate(50,180);
  jugador1.Dibujar();
  popMatrix();
  
  pushMatrix();
  translate(600,180);
  jugador2.Dibujar();
  popMatrix();
  
  
  
  switch (turno){
  case 1:
  keyPressed();
    if((key == 'l') || (key == 'L') ){
      hpjugador2 -= jugador1.ataque; 
      v = hpjugador1;
      turno = 2;
    }
 break;
  case 2:
  keyPressed();
    if((key =='k') || (key == 'K') ){
     hpjugador1 -= jugador2.ataque;
     b = hpjugador2;
     turno = 1;
   }
   break;
  }
  if (hpjugador1 <= 0 ){
    pantalla = 3;
    ganador = 2;
  }
  
  if ( hpjugador2 <= 0){
    pantalla = 3;
    ganador = 1;
  }
  
  if (b <= 0 || v <= 0){
    pantalla = 3;
  }
}

void fin(){
  background(#FFFF81);
  
  if (ganador == 1){
    fill(#FF62AE);
    textSize(50);
   text("Gano el jugador 1", 90,270);
   textSize(20);
   text("Presiona 9 para volver a jugar", 230,400);
  }
  else{
   textSize(50);
   text("Gano el jugador 2", 90,270);
   textSize(20);
   text("Presiona 9 para volver a jugar", 230,400);
  }
  
    keyPressed();
  if (key == '9'){
    pantalla = 0;
    jugador1seleccion = false;
    jugador2seleccion = false;
    hpjugador1 = 100;
    hpjugador2 = 100;
    v = hpjugador1;
    b = hpjugador2;
    turno = 0;
    
  }
  
}



class Personaje{

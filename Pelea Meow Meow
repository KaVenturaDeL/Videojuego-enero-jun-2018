import processing.sound.*;
SoundFile file;


Personaje Personaje1;
Personaje Personaje2;
Personaje Personaje3;
Personaje Personaje4;
Personaje Personaje5;

Personaje Contrincante6;
Personaje Contrincante7;
Personaje Contrincante8;
Personaje Contrincante9;
Personaje Contrincante10;

Personaje jugador1;
Personaje jugador2;

PFont PressStart2PRegular;

int pantalla;
boolean jugador1seleccion = false;
boolean jugador2seleccion = false;
int turno;
int hpjugador1;
int hpjugador2;
int vida = 100;
int v = 100;
int b = 100;
int ganador;


void setup(){
  size(1000,650);
  
  file = new SoundFile(this,"Gatos Kawaii.mp3");
  file.play();
  file.amp(5);
  
  PressStart2PRegular = createFont("PressStart2PRegular.ttf", 10);
  
  Personaje1 = new Personaje("Mazapan",100,10,1);
  Personaje2 = new Personaje("Frenchito",100,10,2);
  Personaje3 = new Personaje("Grey",100,10,3);
  Personaje4 = new Personaje("Thaiga",100,10,4); 
  Personaje5 = new Personaje("Miki",100,10,5); 
  
  Contrincante6 = new Personaje("Mazapan",100,10,1);
  Contrincante7 = new Personaje("Frenchito",100,10,2);
  Contrincante8 = new Personaje("Grey",100,10,3);
  Contrincante9 = new Personaje("Thaiga",100,10,4); 
  Contrincante10 = new Personaje("Miki",100,10,5); 
  
}



void draw(){
  
  println(pantalla);
  
  switch(pantalla){
    case 0:
      intro();
    break;
    case 1:
      seleccion();
    break;
    case 2:
      juego();
    break;
    case 3:
      fin();
    break;
  } 

}

void intro(){
  background(#B774FF);
  fill(0);
  
  textFont(PressStart2PRegular);
  
  fill(#FFBA74);
  textSize(50); //a partir de este las letras aumentan o disminuyen
   text("Pelea Meow Meow", 130,300);   
   fill(#B8FF76);
   textSize(20);
   text("Click aqui para empezar", 280,500); 
if (mousePressed == false){
  } else
  {
    pantalla = 1;
  }
}

